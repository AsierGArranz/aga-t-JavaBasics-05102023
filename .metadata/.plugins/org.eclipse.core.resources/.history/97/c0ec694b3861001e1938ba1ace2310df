package calculosMatematicosEj4;

public class calculosMatematicosEj4App {

	public static void main(String[] args) {
		System.out.println("Ejercicio 4: ");
		int N = 5;
		int mas77= N+77;
		int menos3= mas77-3;
		int dobleresultado = menos3*2;
		
		System.out.println("Valor inicial de N = "+N);
		System.out.println("N + 77 "+mas77);
		System.out.println("N - 3 "+menos3);
		System.out.println("N * 2 "+dobleresultado);

	}

}
