package calculosMatematicosEj2;

public class calculosMatematicosEj2App {

	public static void main(String[] args) {
		System.out.println("Ejercicio 2: ");
		int N = 5;
		double A = 2.5;
		char C = 97;
		
		double suma= N+A;
		double resta= N-A;
		int Cint = C;
		System.out.println("Variable N tiene el valor "+N+" la Variable A tiene el valor "+A+" el Caracter C tiene el valor "+C);
		System.out.println("N + A =  "+suma);
		System.out.println("N - A =  "+resta);
		System.out.println("El valor numerico de C es "+Cint);

		



	}

}
